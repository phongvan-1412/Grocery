{
    "name": "vladelio/pragmaxmlgenerator",
    "description": "A package for Laravel to generate importable XML for Pragma accounting system.",
    "license": "MIT",
    "authors": [
        {
            "name": "Vladislovas Kofyrinas",
            "email": "vladislovas.kofyrinas@gmail.com"
        }
    ],
    "homepage": "https://gitlab.com/Vladelis/laravel-pragma-xml-generator",
    "keywords": ["Laravel", "PragmaXmlGenerator"],
    "require": {
        "illuminate/support": ">=5",
        "spatie/array-to-xml": "^2.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Vladelio\\PragmaXmlGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vladelio\\PragmaXmlGenerator\\PragmaXmlGeneratorServiceProvider"
            ],
            "Vladelio\\PragmaXmlGenerator\\": "packages/Vladelio/PragmaXmlGenerator/src/"
        }
    }

}
